!!!5
%html
  %head
    = stylesheet_link_tag 'screen.css', :media => 'screen, projection'
    = stylesheet_link_tag 'print.css', :media => 'print'
    /[if IE]
      = stylesheet_link_tag 'ie.css', :media => 'screen, projection'
    = javascript_include_tag 'jquery/jquery.min'
    = javascript_include_tag 'jquery/jquery.strftime.min'
    = javascript_include_tag 'js.class/core', 'js.class/observable'
    = javascript_include_tag 'rails'
    = javascript_include_tag 'inflection'
    = javascript_include_tag 'application'
    = render :partial => 'javascript'
    = csrf_meta_tag
    %title Gameroom
  %body
    .main_container
      %header
        %hgroup
          %h1 GAMEROOM
          %h2 1713 O St. Lincoln NE 68508 402-438-7161
        %ul{:id => 'terminal_nav', :class => 'link_nav'}
          %li{:id => 'terminal_nav_user'} Joe Example
          %li &#124;
          %li
            %a{:href => '#dashboard', :class => 'button dashboard'} Dashboard
          %li &#124;
          %li
            %a{:href => '#logout', :class => 'button logout'} Logout
      = yield
    %footer
      = render :partial => 'footer'
    .overlay
    = render :partial => 'modals'